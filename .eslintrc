{
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "semi": false,
        "tabWidth": 2,
        "useTabs": false,
        "printWidth": 120,
        "bracketSpacing": false,
        "bracketSameLine": false,
        "endOfLine": "lf"
      }
    ]
  }
}
